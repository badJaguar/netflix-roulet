{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"indent": false,
		"ordered-imports": [
			true,
			{
				"import-sources-order": "lowercase-last",
				"named-imports-order": "lowercase-first"
			}
		]
	},
	"rulesDirectory": [
		"node_modules/tslint-import-rules/dist/rules"
	]
}